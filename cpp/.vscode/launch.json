{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run cf4pwm (Release)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": [
        "--uri", "radio://0/80/2M",
        "--rate", "500",
        "--affinity", "3",
        "--prio", "high",
        "--spin-tail"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
